<script>

jQuery(document).ready(function($){

	//overlay close panel 
  $('.overlay').on('click', function(event) {
		if (!$(event.target).closest('.overlay-content').length) {
			$('.overlay').remove();
		}
	});

	//button close panel
	$('#shop-promo-website').on('click', function(){
		$('#shop-promo').remove();
  });
	
  //Record
	if ($('#shop-promo').length && typeof(Cookies) == 'object') {
  	if (Cookies.get('shop-promo-cookie') == undefined) {
			$('#shop-promo').addClass('overlay-active');
      		Cookies.set('shop-promo-cookie', '1', { expires: 365, path: '' });
    }
  }
  
  
  
  
  

});
</script></body></html>