<script>

jQuery(document).ready(function($){

	//overlay close panel 
  $('.overlay').on('click', function(event) {
		if (!$(event.target).closest('.overlay-content').length) {
			$('.overlay').remove();
		}
	});

	//button close panel
	$('#shop-promo-website').on('click', function(){
		$('#shop-promo').remove();
  });
	
  //Record
	if ($('#shop-promo').length && typeof(Cookies) == 'object') {
  	if (Cookies.get('shop-promo-cookie') == undefined) {
			$('#shop-promo').addClass('overlay-active');
      		Cookies.set('shop-promo-cookie', '1', { expires: 365, path: '' });
    }
  }
  
  
  
  
  

});
</script><!-- F'in sweet CMS Library for Webflow -->
<script src="https://cdn.finsweet.com/files/cmslibrary-v1.6.js"></script>

<script>
// immediately/self invoked function. This function executes right away
(function() {
  // create a new Library instance and store it in a variable called "customBlogPosts"
  var customBlogPosts = new FsLibrary('.standard-collection-list')

  // define 4 filter groups in an array and store it in a variable called myFilters
var myFilters = [
  {
    filterWrapper: ".filters-wrapper",  // parent wrapper of filter group1
    filterType: "exclusive"
  }
]

  // run the filter Library component on your instance
  customBlogPosts.filter({
    filterArray: myFilters, // variable of the array we created above
    activeClass: 'filter-active', // class that styles the active state (optional)
    animation: {
     enable: true,
     duration: 200,
     easing: 'ease-out'
  }	
  })
})();
</script>



<script>

function getParameterByName(name, url) {
	if (!url) url = window.location.href;
	name = name.replace(/[\[\]]/g, "\\$&");
	var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
	results = regex.exec(url);
	if (!results) return null;
	if (!results[2]) return '';
	return decodeURIComponent(results[2].replace(/\+/g, " "));
	//getParameterByName('var')
}


$(window).on('load', function() {

	if(getParameterByName('filter-by') == 'Christmas') {
		document.getElementById('btn-christmas').click();
	}

});


</script></body></html>